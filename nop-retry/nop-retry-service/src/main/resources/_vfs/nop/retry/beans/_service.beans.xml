<?xml version="1.0" encoding="UTF-8" ?>
<beans x:schema="/nop/schema/beans.xdef" xmlns:x="/nop/schema/xdsl.xdef" xmlns:ioc="ioc"
       xmlns="http://www.springframework.org/schema/beans" x:extends="/nop/biz/defaults/service-base.beans.xml"
       xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
       xsi:schemaLocation="http://www.springframework.org/schema/beans
        http://www.springframework.org/schema/beans/spring-beans-2.5.xsd">

    <bean id="io.nop.retry.service.entity.NopRetryPolicyBizModel" ioc:type="@bean:id" ioc:default="true"/>

    <bean id="biz_NopRetryPolicy" class="io.nop.biz.proxy.BizProxyFactoryBean"
          ioc:type="io.nop.retry.biz.INopRetryPolicyBiz" ioc:force-lazy-property="true" lazy-init="true" primary="true">
        <property name="bizObjName" value="NopRetryPolicy"/>
    </bean>

    <bean id="io.nop.retry.service.entity.NopRetryRecordBizModel" ioc:type="@bean:id" ioc:default="true"/>

    <bean id="biz_NopRetryRecord" class="io.nop.biz.proxy.BizProxyFactoryBean"
          ioc:type="io.nop.retry.biz.INopRetryRecordBiz" ioc:force-lazy-property="true" lazy-init="true" primary="true">
        <property name="bizObjName" value="NopRetryRecord"/>
    </bean>

    <bean id="io.nop.retry.service.entity.NopRetryAttemptBizModel" ioc:type="@bean:id" ioc:default="true"/>

    <bean id="biz_NopRetryAttempt" class="io.nop.biz.proxy.BizProxyFactoryBean"
          ioc:type="io.nop.retry.biz.INopRetryAttemptBiz" ioc:force-lazy-property="true" lazy-init="true" primary="true">
        <property name="bizObjName" value="NopRetryAttempt"/>
    </bean>

    <bean id="io.nop.retry.service.entity.NopRetryDeadLetterBizModel" ioc:type="@bean:id" ioc:default="true"/>

    <bean id="biz_NopRetryDeadLetter" class="io.nop.biz.proxy.BizProxyFactoryBean"
          ioc:type="io.nop.retry.biz.INopRetryDeadLetterBiz" ioc:force-lazy-property="true" lazy-init="true"
          primary="true">
        <property name="bizObjName" value="NopRetryDeadLetter"/>
    </bean>
</beans>