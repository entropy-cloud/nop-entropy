<?xml version="1.0" encoding="UTF-8" ?>
<view x:schema="/nop/schema/xui/xview.xdef" bizObjName="NopRetryDeadLetter" xmlns:i18n-en="i18n-en"
      xmlns:x="/nop/schema/xdsl.xdef" xmlns:j="j" xmlns:c="c" xmlns:gql="gql" xmlns:view-gen="view-gen" xmlns:xpl="xpl">

    <objMeta>/nop/retry/model/NopRetryDeadLetter/NopRetryDeadLetter.xmeta</objMeta>

    <controlLib>/nop/web/xlib/control.xlib</controlLib>

    <x:gen-extends>
        <view-gen:DefaultViewGenExtends xpl:lib="/nop/web/xlib/view-gen.xlib"/>
    </x:gen-extends>

    <x:post-extends>
        <view-gen:DefaultViewPostExtends xpl:lib="/nop/web/xlib/view-gen.xlib"/>
    </x:post-extends>

    <grids>
        <grid id="list" x:abstract="true">
            <cols>

                <!--主键-->
                <col id="sid" mandatory="true" sortable="true"/>

                <!--命名空间ID-->
                <col id="namespaceId" mandatory="true" sortable="true"/>

                <!--组ID-->
                <col id="groupId" mandatory="true" sortable="true"/>

                <!--策略ID-->
                <col id="policyId" sortable="true"/>

                <!--记录ID-->
                <col id="recordId" mandatory="true" sortable="true"/>

                <!--幂等ID-->
                <col id="idempotentId" mandatory="true" sortable="true"/>

                <!--业务号-->
                <col id="bizNo" sortable="true"/>

                <!--执行器名称-->
                <col id="executorName" sortable="true"/>

                <!--请求参数-->
                <col id="requestPayload" sortable="true"/>

                <!--失败码-->
                <col id="failureCode" sortable="true"/>

                <!--失败消息-->
                <col id="failureMessage" sortable="true"/>

                <!--错误堆栈-->
                <col id="errorStack" sortable="true"/>

                <!--最终状态-->
                <col id="finalStatus" ui:number="true" sortable="true"/>

                <!--创建人-->
                <col id="createdBy" sortable="true"/>

                <!--创建时间-->
                <col id="createTime" sortable="true"/>

                <!--更新人-->
                <col id="updatedBy" sortable="true"/>

                <!--更新时间-->
                <col id="updateTime" sortable="true"/>

                <!--服务名-->
                <col id="serviceName" sortable="true"/>

                <!--服务方法-->
                <col id="serviceMethod" sortable="true"/>
            </cols>
        </grid>
        <grid id="pick-list" x:prototype="list" x:abstract="true"/>
    </grids>

    <forms>
        <form id="view" editMode="view" title="查看-重试死信" i18n-en:title="View Retry Dead Letter">
            <layout>
 sid[主键] namespaceId[命名空间ID]
 groupId[组ID] policyId[策略ID]
 recordId[记录ID] idempotentId[幂等ID]
 bizNo[业务号] executorName[执行器名称]
 requestPayload[请求参数] failureCode[失败码]
 failureMessage[失败消息] errorStack[错误堆栈]
 finalStatus[最终状态] createdBy[创建人]
 createTime[创建时间] updatedBy[更新人]
 updateTime[更新时间] serviceName[服务名]
 serviceMethod[服务方法] 
</layout>
        </form>
        <form id="add" editMode="add" title="新增-重试死信" i18n-en:title="Add Retry Dead Letter" x:prototype="edit"/>
        <form id="edit" editMode="edit" title="编辑-重试死信" i18n-en:title="Edit Retry Dead Letter">
            <layout>
 sid[主键] namespaceId[命名空间ID]
 groupId[组ID] policyId[策略ID]
 recordId[记录ID] idempotentId[幂等ID]
 bizNo[业务号] executorName[执行器名称]
 requestPayload[请求参数] failureCode[失败码]
 failureMessage[失败消息] errorStack[错误堆栈]
 finalStatus[最终状态] serviceName[服务名]
 serviceMethod[服务方法] 
</layout>
        </form>
        <form id="query" editMode="query" title="查询条件" i18n-en:title="Query Condition" x:abstract="true"/>
        <form id="asideFilter" editMode="query" x:abstract="true" submitOnChange="true"/>
        <form id="batchUpdate" editMode="edit" x:abstract="true" title="修改-重试死信"
              i18n-en:title="Update Retry Dead Letter"/>
    </forms>

    <pages>
        <crud name="view-list" grid="list" asideFilterForm="asideFilter" filterForm="query" x:abstract="true">
            <table autoFillHeight="true">
                <api url="@query:NopRetryDeadLetter__findPage" gql:selection="{@pageSelection}"/>
            </table>
        </crud>
        <crud name="main" grid="list" asideFilterForm="asideFilter" filterForm="query" x:abstract="true">
            <table autoFillHeight="true">
                <api url="@query:NopRetryDeadLetter__findPage" gql:selection="{@pageSelection}"/>
            </table>
            <listActions>
                <action id="batch-delete-button" label="@i18n:common.batchDelete" batch="true">
                    <api url="@mutation:NopRetryDeadLetter__batchDelete?ids=$ids"/>
                    <confirmText>@i18n:common.confirmBatchDelete</confirmText>
                </action>
                <action id="add-button" level="primary" label="@i18n:common.add" icon="fa fa-plus pull-left">
                    <dialog page="add"/>
                </action>
            </listActions>
            <rowActions>
                <action id="row-view-button" level="primary" label="@i18n:common.view">
                    <dialog page="view"/>
                </action>
                <action id="row-update-button" level="primary" label="@i18n:common.edit" x:abstract="true">
                    <dialog page="update"/>
                </action>
                <action id="row-delete-button" level="danger" label="@i18n:common.delete" x:abstract="true">
                    <api url="@mutation:NopRetryDeadLetter__delete?id=$id"/>
                    <confirmText>@i18n:common.confirmDelete</confirmText>
                </action>
                <actionGroup id="row-more-button" label="@i18n:common.more" level="primary">
                    <action id="row-update-button" label="@i18n:common.edit">
                        <dialog page="update"/>
                    </action>
                    <action id="row-delete-button" label="@i18n:common.delete">
                        <api url="@mutation:NopRetryDeadLetter__delete?id=$id"/>
                        <confirmText>@i18n:common.confirmDelete</confirmText>
                    </action>
                </actionGroup>
            </rowActions>
        </crud>
        <picker name="picker" grid="pick-list" asideFilterForm="asideFilter" filterForm="query" x:abstract="true">
            <table noOperations="true">
                <api url="@query:NopRetryDeadLetter__findPage" gql:selection="{@pageSelection}"/>
            </table>
        </picker>
        <simple name="add" form="add">
            <api url="@mutation:NopRetryDeadLetter__save/id"/>
        </simple>
        <simple name="view" form="view">
            <initApi url="@query:NopRetryDeadLetter__get?id=$id" gql:selection="{@formSelection}"/>
        </simple>
        <simple name="update" form="edit">
            <initApi url="@query:NopRetryDeadLetter__get?id=$id" gql:selection="{@formSelection}"/>
            <api url="@mutation:NopRetryDeadLetter__update/id?id=$id" withFormData="true"/>
        </simple>
    </pages>
</view>