<?xml version="1.0" encoding="UTF-8" ?>
<beans x:schema="/nop/schema/beans.xdef" xmlns:x="/nop/schema/xdsl.xdef" xmlns:ioc="ioc"
       xmlns="http://www.springframework.org/schema/beans" x:extends="/nop/biz/defaults/service-base.beans.xml"
       xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
       xsi:schemaLocation="http://www.springframework.org/schema/beans
        http://www.springframework.org/schema/beans/spring-beans-2.5.xsd">

    <bean id="io.nop.rule.service.entity.NopRuleDefinitionBizModel" ioc:type="@bean:id" ioc:default="true"/>

    <bean id="biz_NopRuleDefinition" class="io.nop.biz.proxy.BizProxyFactoryBean" ioc:type="io.nop.biz.crud.ICrudBiz"
          ioc:force-lazy-property="true">
        <property name="bizObjName" value="NopRuleDefinition"/>
    </bean>

    <bean id="io.nop.rule.service.entity.NopRuleNodeBizModel" ioc:type="@bean:id" ioc:default="true"/>

    <bean id="biz_NopRuleNode" class="io.nop.biz.proxy.BizProxyFactoryBean" ioc:type="io.nop.biz.crud.ICrudBiz"
          ioc:force-lazy-property="true">
        <property name="bizObjName" value="NopRuleNode"/>
    </bean>

    <bean id="io.nop.rule.service.entity.NopRuleRoleBizModel" ioc:type="@bean:id" ioc:default="true"/>

    <bean id="biz_NopRuleRole" class="io.nop.biz.proxy.BizProxyFactoryBean" ioc:type="io.nop.biz.crud.ICrudBiz"
          ioc:force-lazy-property="true">
        <property name="bizObjName" value="NopRuleRole"/>
    </bean>

    <bean id="io.nop.rule.service.entity.NopRuleLogBizModel" ioc:type="@bean:id" ioc:default="true"/>

    <bean id="biz_NopRuleLog" class="io.nop.biz.proxy.BizProxyFactoryBean" ioc:type="io.nop.biz.crud.ICrudBiz"
          ioc:force-lazy-property="true">
        <property name="bizObjName" value="NopRuleLog"/>
    </bean>
</beans>