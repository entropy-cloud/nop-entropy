<?xml version="1.0" encoding="UTF-8" ?>
<beans x:schema="/nop/schema/beans.xdef" xmlns:x="/nop/schema/xdsl.xdef" xmlns:ioc="ioc"
       xmlns="http://www.springframework.org/schema/beans" x:extends="/nop/biz/defaults/service-base.beans.xml"
       xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
       xsi:schemaLocation="http://www.springframework.org/schema/beans
        http://www.springframework.org/schema/beans/spring-beans-2.5.xsd">

    <bean id="io.nop.batch.service.entity.NopBatchFileBizModel" ioc:type="@bean:id" ioc:default="true"/>

    <bean id="biz_NopBatchFile" class="io.nop.biz.proxy.BizProxyFactoryBean"
          ioc:type="io.nop.batch.biz.INopBatchFileBiz" ioc:force-lazy-property="true" lazy-init="true" primary="true">
        <property name="bizObjName" value="NopBatchFile"/>
    </bean>

    <bean id="io.nop.batch.service.entity.NopBatchTaskBizModel" ioc:type="@bean:id" ioc:default="true"/>

    <bean id="biz_NopBatchTask" class="io.nop.biz.proxy.BizProxyFactoryBean"
          ioc:type="io.nop.batch.biz.INopBatchTaskBiz" ioc:force-lazy-property="true" lazy-init="true" primary="true">
        <property name="bizObjName" value="NopBatchTask"/>
    </bean>

    <bean id="io.nop.batch.service.entity.NopBatchTaskVarBizModel" ioc:type="@bean:id" ioc:default="true"/>

    <bean id="biz_NopBatchTaskVar" class="io.nop.biz.proxy.BizProxyFactoryBean"
          ioc:type="io.nop.batch.biz.INopBatchTaskVarBiz" ioc:force-lazy-property="true" lazy-init="true" primary="true">
        <property name="bizObjName" value="NopBatchTaskVar"/>
    </bean>

    <bean id="io.nop.batch.service.entity.NopBatchRecordResultBizModel" ioc:type="@bean:id" ioc:default="true"/>

    <bean id="biz_NopBatchRecordResult" class="io.nop.biz.proxy.BizProxyFactoryBean"
          ioc:type="io.nop.batch.biz.INopBatchRecordResultBiz" ioc:force-lazy-property="true" lazy-init="true"
          primary="true">
        <property name="bizObjName" value="NopBatchRecordResult"/>
    </bean>
</beans>