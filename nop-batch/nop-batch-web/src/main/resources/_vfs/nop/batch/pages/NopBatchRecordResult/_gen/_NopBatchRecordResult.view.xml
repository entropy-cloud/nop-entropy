<?xml version="1.0" encoding="UTF-8" ?>
<view x:schema="/nop/schema/xui/xview.xdef" bizObjName="NopBatchRecordResult" xmlns:i18n-en="i18n-en"
      xmlns:x="/nop/schema/xdsl.xdef" xmlns:j="j">

    <objMeta>/nop/batch/model/NopBatchRecordResult/NopBatchRecordResult.xmeta</objMeta>

    <controlLib>/nop/web/xlib/control.xlib</controlLib>

    <grids>
        <grid id="list" x:abstract="true">
            <cols>

                <!--主键-->
                <col id="taskId" mandatory="true" sortable="true"/>

                <!--记录唯一键-->
                <col id="recordKey" mandatory="true" sortable="true"/>

                <!--返回状态码-->
                <col id="resultStatus" sortable="true"/>

                <!--返回码-->
                <col id="resultCode" sortable="true"/>

                <!--返回消息-->
                <col id="resultMsg" sortable="true"/>

                <!--错误堆栈-->
                <col id="errorStack" sortable="true"/>

                <!--创建人-->
                <col id="createdBy" mandatory="true" sortable="true"/>

                <!--创建时间-->
                <col id="createTime" mandatory="true" sortable="true"/>

                <!--修改人-->
                <col id="updatedBy" mandatory="true" sortable="true"/>

                <!--修改时间-->
                <col id="updateTime" mandatory="true" sortable="true"/>
            </cols>
        </grid>
        <grid id="pick-list" x:prototype="list" x:abstract="true"/>
    </grids>

    <forms>
        <form id="view" editMode="view" title="查看-批处理记录结果" i18n-en:title="View Batch Record Result">
            <layout>
 taskId[主键] recordKey[记录唯一键]
 resultStatus[返回状态码] resultCode[返回码]
 resultMsg[返回消息] errorStack[错误堆栈]
 createdBy[创建人] createTime[创建时间]
 updatedBy[修改人] updateTime[修改时间]
</layout>
        </form>
        <form id="add" editMode="add" title="新增-批处理记录结果" i18n-en:title="Add Batch Record Result" x:prototype="edit"/>
        <form id="edit" editMode="update" title="编辑-批处理记录结果" i18n-en:title="Edit Batch Record Result">
            <layout>
 taskId[主键]
 recordKey[记录唯一键]
 resultStatus[返回状态码]
 resultCode[返回码]
 resultMsg[返回消息]
 errorStack[错误堆栈]
</layout>
        </form>
        <form id="query" editMode="query" title="查询条件" i18n-en:title="Query Condition" x:abstract="true">
            <layout/>
        </form>
        <form id="asideFilter" editMode="query" x:abstract="true" submitOnChange="true">
            <layout/>
        </form>
        <form id="batchUpdate" editMode="update" x:abstract="true" title="修改-批处理记录结果"
              i18n-en:title="Update Batch Record Result">
            <layout/>
        </form>
    </forms>

    <pages>
        <crud name="main" grid="list" asideFilterForm="asideFilter" filterForm="query" x:abstract="true">
            <table autoFillHeight="true">
                <api url="@query:NopBatchRecordResult__findPage/{@pageSelection}"/>
            </table>
            <listActions>
                <action id="batch-delete-button" label="@i18n:common.batchDelete" batch="true">
                    <api url="@mutation:NopBatchRecordResult__delete?ids=$ids"/>
                    <confirmText>@i18n:common.confirmBatchDelete</confirmText>
                </action>
                <action id="add-button" level="primary" label="@i18n:common.add" icon="fa fa-plus pull-left">
                    <dialog page="add"/>
                </action>
            </listActions>
            <rowActions>
                <action id="row-view-button" level="primary" label="@i18n:common.view">
                    <dialog page="view"/>
                </action>
                <action id="row-update-button" level="primary" label="@i18n:common.edit" x:abstract="true">
                    <dialog page="update"/>
                </action>
                <action id="row-delete-button" level="danger" label="@i18n:common.delete" x:abstract="true">
                    <api url="@mutation:NopBatchRecordResult__delete?id=$id"/>
                    <confirmText>@i18n:common.confirmDelete</confirmText>
                </action>
                <actionGroup id="row-more-button" label="@i18n:common.more" level="primary">
                    <action id="row-update-button" level="primary" label="@i18n:common.edit">
                        <dialog page="update"/>
                    </action>
                    <action id="row-delete-button" level="danger" label="@i18n:common.delete">
                        <api url="@mutation:NopBatchRecordResult__delete?id=$id"/>
                        <confirmText>@i18n:common.confirmDelete</confirmText>
                    </action>
                </actionGroup>
            </rowActions>
        </crud>
        <picker name="picker" grid="pick-list" asideFilterForm="asideFilter" filterForm="query" x:abstract="true">
            <table>
                <api url="@query:NopBatchRecordResult__findPage/{@pageSelection}"/>
            </table>
        </picker>
        <simple name="add" form="add">
            <api url="@mutation:NopBatchRecordResult__save/id"/>
        </simple>
        <simple name="view" form="view">
            <initApi url="@query:NopBatchRecordResult__get/{@formSelection}?id=$id"/>
        </simple>
        <simple name="update" form="edit">
            <initApi url="@query:NopBatchRecordResult__get/{@formSelection}?id=$id"/>
            <api url="@mutation:NopBatchRecordResult__update/id?id=$id" withFormData="true"/>
        </simple>
    </pages>
</view>