<?xml version="1.0" encoding="UTF-8" ?>
<view x:schema="/nop/schema/xui/xview.xdef" bizObjName="NopSysSequence" xmlns:i18n-en="i18n-en"
      xmlns:x="/nop/schema/xdsl.xdef" xmlns:j="j">

    <objMeta>/nop/sys/model/NopSysSequence/NopSysSequence.xmeta</objMeta>

    <controlLib>/nop/web/xlib/control.xlib</controlLib>

    <grids>
        <grid id="list" x:abstract="true">
            <cols>

                <!--名称-->
                <col id="seqName" mandatory="true" sortable="true"/>

                <!--类型-->
                <col id="seqType" sortable="true"/>

                <!--是否UUID-->
                <col id="isUuid" mandatory="true" sortable="true"/>

                <!--下一个值-->
                <col id="nextValue" mandatory="true" sortable="true"/>

                <!--步长-->
                <col id="stepSize" mandatory="true" sortable="true"/>

                <!--缓存个数-->
                <col id="cacheSize" sortable="true"/>

                <!--最大值-->
                <col id="maxValue" sortable="true"/>

                <!--重置方式-->
                <col id="resetType" sortable="true"/>

                <!--删除标识-->
                <col id="delFlag" mandatory="true" sortable="true"/>

                <!--创建人-->
                <col id="createdBy" mandatory="true" sortable="true"/>

                <!--创建时间-->
                <col id="createTime" mandatory="true" sortable="true"/>

                <!--修改人-->
                <col id="updatedBy" mandatory="true" sortable="true"/>

                <!--修改时间-->
                <col id="updateTime" mandatory="true" sortable="true"/>

                <!--备注-->
                <col id="remark" sortable="true"/>
            </cols>
        </grid>
        <grid id="pick-list" x:prototype="list" x:abstract="true"/>
    </grids>

    <forms>
        <form id="view" editMode="view" title="查看-序列号" i18n-en:title="View Sequence">
            <layout>
 seqName[名称] seqType[类型]
 isUuid[是否UUID] nextValue[下一个值]
 stepSize[步长] cacheSize[缓存个数]
 maxValue[最大值] resetType[重置方式]
 delFlag[删除标识] createdBy[创建人]
 createTime[创建时间] updatedBy[修改人]
 updateTime[修改时间] remark[备注]
</layout>
        </form>
        <form id="add" editMode="add" title="新增-序列号" i18n-en:title="Add Sequence" x:prototype="edit"/>
        <form id="edit" editMode="update" title="编辑-序列号" i18n-en:title="Edit Sequence">
            <layout>
 seqName[名称] seqType[类型]
 isUuid[是否UUID] nextValue[下一个值]
 stepSize[步长] cacheSize[缓存个数]
 maxValue[最大值] resetType[重置方式]
 delFlag[删除标识] remark[备注]
</layout>
        </form>
        <form id="query" editMode="query" title="查询条件" i18n-en:title="Query Condition" x:abstract="true">
            <layout/>
        </form>
        <form id="asideFilter" editMode="query" x:abstract="true" submitOnChange="true">
            <layout/>
        </form>
        <form id="batchUpdate" editMode="update" x:abstract="true" title="修改-序列号" i18n-en:title="Update Sequence">
            <layout/>
        </form>
    </forms>

    <pages>
        <crud name="main" grid="list" asideFilterForm="asideFilter" filterForm="query" x:abstract="true">
            <table autoFillHeight="true">
                <api url="@query:NopSysSequence__findPage/{@pageSelection}"/>
            </table>
            <listActions>
                <action id="batch-delete-button" label="@i18n:common.batchDelete" batch="true">
                    <api url="@mutation:NopSysSequence__delete?ids=$ids"/>
                    <confirmText>@i18n:common.confirmBatchDelete</confirmText>
                </action>
                <action id="add-button" level="primary" label="@i18n:common.add" icon="fa fa-plus pull-left">
                    <dialog page="add"/>
                </action>
            </listActions>
            <rowActions>
                <action id="row-view-button" level="primary" label="@i18n:common.view">
                    <dialog page="view"/>
                </action>
                <action id="row-update-button" level="primary" label="@i18n:common.edit" x:abstract="true">
                    <dialog page="update"/>
                </action>
                <action id="row-delete-button" level="danger" label="@i18n:common.delete" x:abstract="true">
                    <api url="@mutation:NopSysSequence__delete?id=$id"/>
                    <confirmText>@i18n:common.confirmDelete</confirmText>
                </action>
                <actionGroup id="row-more-button" label="@i18n:common.more" level="primary">
                    <action id="row-update-button" level="primary" label="@i18n:common.edit">
                        <dialog page="update"/>
                    </action>
                    <action id="row-delete-button" level="danger" label="@i18n:common.delete">
                        <api url="@mutation:NopSysSequence__delete?id=$id"/>
                        <confirmText>@i18n:common.confirmDelete</confirmText>
                    </action>
                </actionGroup>
            </rowActions>
        </crud>
        <picker name="picker" grid="pick-list" asideFilterForm="asideFilter" filterForm="query" x:abstract="true">
            <table>
                <api url="@query:NopSysSequence__findPage/{@pageSelection}"/>
            </table>
        </picker>
        <simple name="add" form="add">
            <api url="@mutation:NopSysSequence__save/id"/>
        </simple>
        <simple name="view" form="view">
            <api url="@query:NopSysSequence__get/{@formSelection}?id=$id"/>
        </simple>
        <simple name="update" form="edit">
            <initApi url="@query:NopSysSequence__get/{@formSelection}?id=$id"/>
            <api url="@mutation:NopSysSequence__update/id?id=$id" withFormData="true"/>
        </simple>
    </pages>
</view>