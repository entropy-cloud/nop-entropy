<?xml version="1.0" encoding="UTF-8" ?>
<beans x:schema="/nop/schema/beans.xdef" xmlns:x="/nop/schema/xdsl.xdef" xmlns:ioc="ioc"
       xmlns="http://www.springframework.org/schema/beans" x:extends="/nop/biz/defaults/service-base.beans.xml"
       xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
       xsi:schemaLocation="http://www.springframework.org/schema/beans
        http://www.springframework.org/schema/beans/spring-beans-2.5.xsd">

    <bean id="io.nop.job.service.entity.NopJobDefinitionBizModel" ioc:type="@bean:id" ioc:default="true"/>

    <bean id="biz_NopJobDefinition" class="io.nop.biz.proxy.BizProxyFactoryBean" ioc:type="io.nop.biz.crud.ICrudBiz"
          ioc:force-lazy-property="true">
        <property name="bizObjName" value="NopJobDefinition"/>
    </bean>

    <bean id="io.nop.job.service.entity.NopJobAssignmentBizModel" ioc:type="@bean:id" ioc:default="true"/>

    <bean id="biz_NopJobAssignment" class="io.nop.biz.proxy.BizProxyFactoryBean" ioc:type="io.nop.biz.crud.ICrudBiz"
          ioc:force-lazy-property="true">
        <property name="bizObjName" value="NopJobAssignment"/>
    </bean>

    <bean id="io.nop.job.service.entity.NopJobInstanceHisBizModel" ioc:type="@bean:id" ioc:default="true"/>

    <bean id="biz_NopJobInstanceHis" class="io.nop.biz.proxy.BizProxyFactoryBean" ioc:type="io.nop.biz.crud.ICrudBiz"
          ioc:force-lazy-property="true">
        <property name="bizObjName" value="NopJobInstanceHis"/>
    </bean>

    <bean id="io.nop.job.service.entity.NopJobInstanceBizModel" ioc:type="@bean:id" ioc:default="true"/>

    <bean id="biz_NopJobInstance" class="io.nop.biz.proxy.BizProxyFactoryBean" ioc:type="io.nop.biz.crud.ICrudBiz"
          ioc:force-lazy-property="true">
        <property name="bizObjName" value="NopJobInstance"/>
    </bean>
</beans>