<?xml version="1.0" encoding="UTF-8" ?>
<beans x:schema="/nop/schema/beans.xdef" xmlns:x="/nop/schema/xdsl.xdef" xmlns:ioc="ioc"
       xmlns="http://www.springframework.org/schema/beans" x:extends="/nop/biz/defaults/service-base.beans.xml"
       xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
       xsi:schemaLocation="http://www.springframework.org/schema/beans
        http://www.springframework.org/schema/beans/spring-beans-2.5.xsd">

    <bean id="io.nop.oauth.service.entity.NopOauthAuthorizationBizModel" ioc:type="@bean:id" ioc:default="true"/>

    <bean id="biz_NopOauthAuthorization" class="io.nop.biz.proxy.BizProxyFactoryBean"
          ioc:type="io.nop.oauth.biz.INopOauthAuthorizationBiz" ioc:force-lazy-property="true" lazy-init="true"
          primary="true">
        <property name="bizObjName" value="NopOauthAuthorization"/>
    </bean>

    <bean id="io.nop.oauth.service.entity.NopOauthAuthorizationConsentBizModel" ioc:type="@bean:id" ioc:default="true"/>

    <bean id="biz_NopOauthAuthorizationConsent" class="io.nop.biz.proxy.BizProxyFactoryBean"
          ioc:type="io.nop.oauth.biz.INopOauthAuthorizationConsentBiz" ioc:force-lazy-property="true" lazy-init="true"
          primary="true">
        <property name="bizObjName" value="NopOauthAuthorizationConsent"/>
    </bean>

    <bean id="io.nop.oauth.service.entity.NopOauthRegisteredClientBizModel" ioc:type="@bean:id" ioc:default="true"/>

    <bean id="biz_NopOauthRegisteredClient" class="io.nop.biz.proxy.BizProxyFactoryBean"
          ioc:type="io.nop.oauth.biz.INopOauthRegisteredClientBiz" ioc:force-lazy-property="true" lazy-init="true"
          primary="true">
        <property name="bizObjName" value="NopOauthRegisteredClient"/>
    </bean>
</beans>