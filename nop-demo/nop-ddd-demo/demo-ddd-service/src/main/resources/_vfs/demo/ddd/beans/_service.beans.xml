<?xml version="1.0" encoding="UTF-8" ?>
<beans x:schema="/nop/schema/beans.xdef" xmlns:x="/nop/schema/xdsl.xdef" xmlns:ioc="ioc"
       xmlns="http://www.springframework.org/schema/beans" x:extends="/nop/biz/defaults/service-base.beans.xml"
       xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
       xsi:schemaLocation="http://www.springframework.org/schema/beans
        http://www.springframework.org/schema/beans/spring-beans-2.5.xsd">

    <bean id="app.demo.service.entity.VoyageBizModel" ioc:type="@bean:id" ioc:default="true"/>

    <bean id="biz_Voyage" class="io.nop.biz.proxy.BizProxyFactoryBean" ioc:type="app.demo.biz.IVoyageBiz"
          ioc:force-lazy-property="true" lazy-init="true" primary="true">
        <property name="bizObjName" value="Voyage"/>
    </bean>

    <bean id="app.demo.service.entity.LocationBizModel" ioc:type="@bean:id" ioc:default="true"/>

    <bean id="biz_Location" class="io.nop.biz.proxy.BizProxyFactoryBean" ioc:type="app.demo.biz.ILocationBiz"
          ioc:force-lazy-property="true" lazy-init="true" primary="true">
        <property name="bizObjName" value="Location"/>
    </bean>

    <bean id="app.demo.service.entity.CargoBizModel" ioc:type="@bean:id" ioc:default="true"/>

    <bean id="biz_Cargo" class="io.nop.biz.proxy.BizProxyFactoryBean" ioc:type="app.demo.biz.ICargoBiz"
          ioc:force-lazy-property="true" lazy-init="true" primary="true">
        <property name="bizObjName" value="Cargo"/>
    </bean>

    <bean id="app.demo.service.entity.CarrierMovementBizModel" ioc:type="@bean:id" ioc:default="true"/>

    <bean id="biz_CarrierMovement" class="io.nop.biz.proxy.BizProxyFactoryBean"
          ioc:type="app.demo.biz.ICarrierMovementBiz" ioc:force-lazy-property="true" lazy-init="true" primary="true">
        <property name="bizObjName" value="CarrierMovement"/>
    </bean>

    <bean id="app.demo.service.entity.HandlingEventBizModel" ioc:type="@bean:id" ioc:default="true"/>

    <bean id="biz_HandlingEvent" class="io.nop.biz.proxy.BizProxyFactoryBean" ioc:type="app.demo.biz.IHandlingEventBiz"
          ioc:force-lazy-property="true" lazy-init="true" primary="true">
        <property name="bizObjName" value="HandlingEvent"/>
    </bean>

    <bean id="app.demo.service.entity.LegBizModel" ioc:type="@bean:id" ioc:default="true"/>

    <bean id="biz_Leg" class="io.nop.biz.proxy.BizProxyFactoryBean" ioc:type="app.demo.biz.ILegBiz"
          ioc:force-lazy-property="true" lazy-init="true" primary="true">
        <property name="bizObjName" value="Leg"/>
    </bean>
</beans>